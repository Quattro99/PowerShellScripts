                    "Microsoft.KeyVault/vaults/secrets/read",
                    "Microsoft.KeyVault/vaults/secrets/list",
                    "Microsoft.KeyVault/vaults/secrets/backup/action"
                ],
                "notActions":[
                    
                ],
                "dataActions": [
                    
                ],
                "notDataActions": [
                    
                ]
            }
        ]
    }
}
